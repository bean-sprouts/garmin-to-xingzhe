{
  "name": "佳明骑行记录导入行者",
  "description": "佳明骑行记录导入行者",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "image/icon16.png"
  },
  "background": {
    "scripts": ["js/jszip.min.js","js/background.js"],
    "persistent": false
  },
  "permissions": [
    "windows",
    "alarms",
    "notifications",
    "storage",
    "https://www.imxingzhe.com/*",
    "https://connect.garmin.cn/*"
  ]
}
